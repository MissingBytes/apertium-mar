#Det if succeeded by N or Adj
SELECT Det IF
    (0 Det)
    (0 Prn)
    (1 N) ;

SELECT Det IF
    (0 Det)
    (0 Prn)
    (1 Adj)
    (2 N) ;

#Otherwise, choose Prn
SELECT Prn IF
    (0 Det)
    (0 Prn) ;


